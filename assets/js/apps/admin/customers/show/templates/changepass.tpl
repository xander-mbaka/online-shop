<div class="archives">
<ul class="jsub-tab">
        <li class="jsubsec left">
          <h6>YOUR ACCOUNT INFORMATION</h6>
          <form>
            <div class="labForm-group">
              <label class="lab-label" for="lab-sname">EMAIL</label>
              <input id="lab-sname" class="lab-input" type="email" name="email" value="<%= email %>" disabled="true">
            </div>
            <h5>CHANGE PASSWORD:</h5>
            <div class="labForm-group">
              <label class="lab-label" for="lab-ssopword">OLD PASSWORD</label>
              <input id="lab-ssopword" class="lab-input" type="password" name="oldpassword" value="">
            </div>
            <div class="labForm-group">
              <label class="lab-label" for="lab-ssnpword">NEW PASSWORD</label>
              <input id="lab-ssnpword" class="lab-input" type="password" name="newpassword" value="">
            </div>
            <div class="labForm-group">
              <label class="lab-label" for="lab-ssrnpword">REPEAT NEW PASSWORD</label>
              <input id="lab-ssrnpword" class="lab-input" type="password" name="rptpassword" value="">
            </div>
            <button class="btn btn-primary btn-small csubmit">CHANGE PASSWORD</button>
          </form>
        </li>
      </ul>
    </div>